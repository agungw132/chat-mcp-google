# Google account email used by MCP Gmail/Calendar/Contacts
GOOGLE_ACCOUNT=your-email@gmail.com

# Google App Password (16 characters, no spaces) for IMAP/SMTP/CalDAV/CardDAV access
GOOGLE_APP_KEY=xxxxxxxxxxxxxxxx

# Google Drive OAuth 2.0 access token (Bearer token), required by Drive MCP
# Note: this is not Google App Password and usually expires (commonly around 1 hour)
GOOGLE_DRIVE_ACCESS_TOKEN=your-drive-access-token

# Gemini API key for models whose names start with 'gemini'
GOOGLE_GEMINI_API_KEY=your-gemini-api-key

# OpenAI-compatible base URL for non-Gemini models
BASE_URL=https://ai.sumopod.com

# API key for BASE_URL (Bearer token)
API_KEY=your-openai-compatible-api-key

# Initial default model for the UI dropdown (must exist in AVAILABLE_MODELS)
MODEL=azure_ai/kimi-k2.5
